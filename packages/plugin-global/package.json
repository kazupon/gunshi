{
  "name": "@gunshi/plugin-global",
  "description": "global options plugin for gunshi",
  "version": "0.26.3",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/gunshi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/gunshi.git",
    "directory": "packages/plugin-global"
  },
  "keywords": [
    "gunshi",
    "plugin",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 20"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "pnpm run build:typedoc && pnpm run build:plugin",
    "build:plugin": "tsdown",
    "build:typedoc": "typedoc --excludeInternal",
    "lint:jsr": "jsr publish --dry-run --allow-dirty",
    "prepack": "pnpm build",
    "typecheck:deno": "deno check ./src"
  },
  "dependencies": {
    "@gunshi/plugin": "workspace:*"
  },
  "devDependencies": {
    "@gunshi/shared": "workspace:*",
    "deno": "catalog:",
    "jsr": "catalog:",
    "jsr-exports-lint": "catalog:",
    "publint": "catalog:",
    "tsdown": "catalog:",
    "typedoc": "catalog:",
    "typedoc-plugin-markdown": "catalog:"
  }
}
