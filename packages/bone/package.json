{
  "name": "@gunshi/bone",
  "description": "gunshi minimum",
  "version": "0.27.0-beta.2",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/gunshi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/gunshi.git",
    "directory": "packages/bone"
  },
  "keywords": [
    "gunshi",
    "plugin",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 20"
  },
  "type": "module",
  "files": [
    "lib"
  ],
  "module": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint:jsr": "jsr publish --dry-run --allow-dirty",
    "prepack": "pnpm build",
    "typecheck:deno": "deno check --import-map=../../importmap.json ./src"
  },
  "devDependencies": {
    "@gunshi/definition": "workspace:*",
    "@gunshi/plugin-global": "workspace:*",
    "@gunshi/plugin-renderer": "workspace:*",
    "deno": "catalog:",
    "gunshi": "workspace:*",
    "jsr": "catalog:",
    "jsr-exports-lint": "catalog:",
    "publint": "catalog:",
    "tsdown": "catalog:"
  }
}
