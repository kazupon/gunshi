{
  "$id": "https://jsr.io/schema/config-file.v1.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "name": "@gunshi/shared",
  "description": "shared utils for gunshi",
  "version": "0.27.0-beta.2",
  "license": "MIT",
  "publish": {
    "include": ["src/**/*.ts", "package.json", "jsr.json", "README.md", "CHANGELOG.md", "LICENSE"],
    "exclude": [
      "examples/**/*.ts",
      "src/**/*.test.ts",
      "src/**/*.test-d.ts",
      "**/__snapshots__/**",
      "**/lib/**",
      "**/dist/**"
    ]
  },
  "exports": {
    ".": "./src/index.ts"
  }
}
